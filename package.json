{
  "name": "alexa-countryquiz-skill",
  "version": "0.0.0",
  "description": "An Alexa skill for the Amazon Echo.",
  "main": "build/index.js",
  "scripts": {
    "lint": "eslint src/**/*.js test/**/*.js",
    "generate-data": "bin/generate-data --destination src/",
    "generate-dictionary": "bin/generate-dictionary --destination model/",
    "prebuild": "npm run generate-dictionary",
    "build": "npm-run-all --parallel build:skill build:utterances build:slot-values",
    "build:skill": "webpack --config config/webpack.config.babel.js",
    "build:utterances": "mkdir -p build && bin/utterances --schema model/schema.json --utterances model/UTTERANCES > build/UTTERANCES",
    "build:slot-values": "mkdir -p build && bin/slot-values --destination build/",
    "pretest": "npm run build",
    "test": "ava",
    "posttest": "npm run lint",
    "deploy:prod": "serverless deploy --stage production",
    "deploy:dev": "serverless deploy function -f skill"
  },
  "ava": {
    "babel": "inherit",
    "failWithoutAssertions": false
  },
  "devDependencies": {
    "alexa-annotations": "^9.0.1",
    "alexa-request": "^0.3.3",
    "alexa-response": "^4.0.1",
    "alexa-ssml-jsx": "^2.0.0",
    "alexa-utterances": "^0.2.1",
    "ava": "^0.20.0",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.7.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "babili-webpack-plugin": "^0.1.2",
    "bestzip": "^1.1.3",
    "chai": "^4.0.0",
    "chai-subset": "^1.5.0",
    "cheerio": "^1.0.0-rc.1",
    "countryjs": "^1.8.0",
    "eslint": "^4.1.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "node-aws-lambda": "^0.1.8",
    "nomnom": "^1.8.1",
    "npm-run-all": "^4.0.0",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.3",
    "serverless": "^1.10.1",
    "svg-country-flags": "^1.0.0",
    "webpack": "^3.0.0"
  },
  "files": [
    "build",
    "src"
  ],
  "keywords": [
    "alexa",
    "alexa-skill",
    "echo",
    "skill"
  ]
}
